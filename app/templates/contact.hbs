<h1>{{model.first_name}} {{model.last_name}}</h1>

<dl>
  <dt>Email</dt>
  <dd>{{model.email}}</dd>

  <dt>Title</dt>
  <dd>{{model.title}}</dd>
</dl>

<p>
  Updated at: {{formatted-date updatedAt 'MMMM Do, YYYY [at] h:mm a'}}
  <br>
  Created at: {{formatted-date createdAt 'MMMM Do, YYYY [at] h:mm a'}}
</p>
